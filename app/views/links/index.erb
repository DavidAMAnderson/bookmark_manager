<h1> Links </h1>

<form action="/links/new">
  <input type="submit" value="Create New Bookmark">
</form>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL:   <%= link.url   %>
      Tags: <% link.tags.each do |tag| %>
        <%= tag.name %>
      <% end %>
    </li>
  <% end %>
</ul>
